{
  "name": "lkt-string-tools",
  "version": "1.0.1",
  "description": "A collection of functions for working with strings",
  "main": "src/index.js",
  "module": "src/index.js",
  "keywords": [
    "lkt web tech",
    "ts",
    "typescript",
    "js",
    "javascript"
  ],
  "files": [
    "dist/*"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/lkt-string-tools.es.js",
      "require": "./dist/lkt-string-tools.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "serve": "vite",
    "build": "vite build && vue-tsc --emitDeclarationOnly --project tsconfig.json && mv dist/src dist/types && rm dist/jest.config.d.ts && rm -rf dist/tests && rm dist/vite.config.d.ts && rm dist/README.md.d.ts",
    "preview": "vite preview",
    "test": "jest",
    "test-coverage": "vitest run --coverage",
    "lint": "eslint --ignore-path /dist --ignore-path .gitignore . --ext .ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/lkt-ui/lkt-string-tools.git"
  },
  "author": "Antonio Ibáñez <lkt.web.tech@gmail.com>",
  "license": "MIT",
  "homepage": "https://gitlab.com/lkt-ui/lkt-string-tools#readme",
  "bugs": {
    "url": "https://gitlab.com/lkt-ui/lkt-string-tools/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.13.13",
    "@babel/preset-env": "^7.13.12",
    "@babel/types": "^7.18.9",
    "@testing-library/cypress": "^7.0.4",
    "@testing-library/dom": "^7.30.1",
    "@testing-library/jest-dom": "^5.11.10",
    "@types/jest": "^29.0.1",
    "@types/node": "^18.6.2",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "@vitejs/plugin-vue": "^3.0.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^29.0.3",
    "babel-loader": "^8.2.2",
    "cypress": "^6.8.0",
    "eslint": "8.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "jest": "^29.0.3",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.0",
    "typescript": "^4.8.3",
    "vue": "^3.2.37",
    "vue-tsc": "^0.38.9"
  },
  "peerDependencies": {
    "lkt-ts-interfaces": "^1.0.0"
  }
}
